<div id="<%= dom_id friend %>">
  <tr scope="row">
    <td>
      <%= link_to friend.first_name + " " + friend.last_name, friend, class:"text-decoration-none" %>
    </td>
    <td>
      <%= friend.phone %>
    </td>
    <td>
      <%= friend.email %>
    </td>
    <td>
      <%= friend.user_id %>
    </td>
    <td>
      <%= button_to "Delete", friend, form: { data: { turbo_confirm: "Are you sure?" }}, method: :delete, class:"btn btn-outline-danger btn-sm"%>
    </td> 
  </tr>
</div>
